{
   "compilerOptions": {
      "target": "ESNext",
      "strict": true,
      "sourceMap": true,
      "allowJs": true,
      "removeComments": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "dist"
   },
   "include": ["src"],
   "exclude": ["node_modules"]
}
